<!DOCTYPE html>
<html>
<link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@600&display=swap" rel="stylesheet">
<link href="/css/appointmentmoreinfo.css" rel="stylesheet" type="text/css">
<head>

	<title>app info</title>
</head>

<body>
	<img src="https://static1.squarespace.com/static/58c6fc4a37c581154f75cdd2/t/58e662456a496356f0430f90/1604006272177/">
	<div id="container">
		<div id="head">
			<h2>Appointments</h2>
		</div>
		<table border="1">
	
			<th>S no</th>
			<th>First name</th>
			<th>Last name</th>
			<th>Health Issue</th>
			<th>Phone number</th>
			<th>Email</th>

			<% var i=1;
	        if(user25.length!=0){
	        user25.forEach(function(data){
	        %>
			<tr>
				<td><%=i%></td>
				<td><%=data.fn%></td>
				<td><%=data.ln%></td>
				<td><%=data.prob%></td>
				<td><%=data.pno%></td>
				<td><%=data.email%></td>
			</tr>
			<% i++; });}%>
		</table>
		<div id="hos">
			<strong><%=hos%></strong>
		</div>
	</div>
</body>

</html>
